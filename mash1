	<body style="background-color:lightblue;">
		<script type="text/javascript">
			function bigImg(x)
				{
					x.style.height="512px";
					x.style.width="512px";
				}
			function normalImg(x)
				{
					x.style.height="256px";
					x.style.width="256px";
				}
		</script>
		<img style="float:right;" onmouseover="bigImg(this)" onmouseout="normalImg(this)" border="0" src="mash.png" alt="Mashhad" width="256" height="256" />
	</body>
